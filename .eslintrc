{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "no-tabs": 0,
        "indent": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "react/prop-types": 0,
        "no-console": 0
    },
    "globals": {
        "fetch": true
    }
}
